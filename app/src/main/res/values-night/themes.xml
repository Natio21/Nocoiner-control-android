<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Nocoinercontrol" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/natio_grey_80</item>
        <item name="colorPrimaryVariant">@color/natio_grey_40</item>
        <item name="colorOnPrimary">@color/natio_black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/natio_orange_80</item>
        <item name="colorSecondaryVariant">@color/natio_black_40</item>
        <item name="colorOnSecondary">@color/natio_white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.Nocoinercontrol.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Nocoinercontrol.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Nocoinercontrol.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>